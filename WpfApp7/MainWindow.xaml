<Window x:Class="WpfApp7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp7"
        mc:Ignorable="d"
        Title="Fxod" MinHeight="280" MinWidth="600" MaxHeight="450" MaxWidth="900"
        SizeToContent="WidthAndHeight" FontFamily="Verdana" Icon="/Image\Icon.png">
    <Grid>
        <StackPanel Margin="10">
            <Label Background="Aqua" HorizontalContentAlignment="Center" Margin="5">Login</Label>
            <TextBox x:Name="tbLogin" Margin="5" Background="AntiqueWhite"></TextBox>

            <Label Background="Aqua" HorizontalContentAlignment="Center" Margin="5">Password</Label>
            <TextBox x:Name="tbPassword" Margin="5" Background="AntiqueWhite"></TextBox>

            <Button Margin="10" x:Name="btGo" Click="btGo_Click" Background="Aqua">Go</Button>
            <Button Margin="10" x:Name="btExist" Click="btExist_Click"  Background="Aqua">Exist</Button>

        </StackPanel>
    </Grid>
</Window>
